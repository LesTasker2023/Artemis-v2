{
  "name": "artemis-v2",
  "version": "2.0.0-alpha.20",
  "description": "ARTEMIS v2 - TypeScript rewrite with immutable architecture",
  "type": "module",
  "main": "dist-electron/main.cjs",
  "author": "Les Tasker",
  "repository": {
    "type": "git",
    "url": "https://github.com/LesTasker2023/Artemis-v2.git"
  },
  "scripts": {
    "dev": "vite",
    "dev:electron": "concurrently -k \"vite\" \"npm run electron:dev\"",
    "electron:dev": "node build-electron.js && wait-on tcp:3000 && cross-env VITE_DEV_SERVER_URL=http://localhost:3000 electron .",
    "build": "tsc && vite build",
    "build:electron": "npm run build && electron-builder",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint . --ext ts,tsx",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@kobalte/core": "^0.12.0",
    "@solidjs/router": "^0.15.4",
    "better-sqlite3": "^12.4.1",
    "chart.js": "^4.5.1",
    "clsx": "^2.0.0",
    "date-fns": "^3.0.0",
    "effect": "^2.0.0",
    "electron-updater": "^6.6.2",
    "fp-ts": "^2.16.0",
    "immer": "^10.0.0",
    "kysely": "^0.27.6",
    "lucide-solid": "^0.294.0",
    "mapbox-gl": "^3.0.0",
    "recharts": "^3.4.1",
    "solid-chartjs": "^1.3.11",
    "solid-js": "^1.8.0",
    "tailwind-merge": "^2.0.0",
    "ts-pattern": "^5.0.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@electron/rebuild": "^4.0.1",
    "@types/better-sqlite3": "^7.6.13",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "@vitest/ui": "^1.0.0",
    "autoprefixer": "^10.4.0",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.0",
    "electron-rebuild": "^3.2.9",
    "eslint": "^8.56.0",
    "eslint-plugin-solid": "^0.13.0",
    "jsdom": "^27.2.0",
    "postcss": "^8.4.0",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-electron-renderer": "^0.14.6",
    "vite-plugin-solid": "^2.8.0",
    "vitest": "^1.0.0",
    "wait-on": "^9.0.3"
  }
}